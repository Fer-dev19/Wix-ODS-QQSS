<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/sostenibleregistro">

    <TextView
        android:id="@+id/textViewNotBad"
        android:layout_width="315dp"
        android:layout_height="53dp"
        android:layout_marginStart="60dp"
        android:layout_marginTop="140dp"
        android:text="No está mal"
        android:textAlignment="center"
        android:textAppearance="@style/TextAppearance.AppCompat.Body1"
        android:textSize="38sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView40"
        android:layout_width="228dp"
        android:layout_height="50dp"
        android:layout_marginStart="60dp"
        android:layout_marginTop="44dp"
        android:text="Puntuación consolidada:"
        android:textSize="20sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textViewNotBad" />

    <TextView
        android:id="@+id/textView29"
        android:layout_width="89dp"
        android:layout_height="57dp"
        android:layout_marginStart="5dp"
        android:layout_marginTop="44dp"
        android:text="2000"
        android:textSize="20sp"
        app:layout_constraintStart_toEndOf="@+id/textView40"
        app:layout_constraintTop_toBottomOf="@+id/textViewNotBad" />

    <Button
        android:id="@+id/buttonContinuarAlMenu"
        android:layout_width="207dp"
        android:layout_height="65dp"
        android:layout_marginStart="100dp"
        android:layout_marginTop="20dp"
        android:onClick="irAlMenu"
        android:text="Volver al menú"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView40" />

</androidx.constraintlayout.widget.ConstraintLayout>